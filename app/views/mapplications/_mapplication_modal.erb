<div class="modal-header">
  <h3 class="modal-title"><a href="/accounts/<%= mapplication.account_id %>"><%= mapplication.account.name %></a>'s application to <%= mapplication.gathering.name %></h3>
  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<div class="modal-body">
  <%= partial :'mapplications/mapplication', locals: { mapplication: mapplication } %>
</div>
